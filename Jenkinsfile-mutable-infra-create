pipeline {
agent {
  label 'WORKSTATION'
}
  options {
    ansiColor('xterm')
  }

 stages {

    stage('Repo Clone') {
      steps {
        dir('CART') {
          git branch: 'main', url: 'https://github.com/kana-Gayatri/cart.git'
        }
      }
    }




        stage('CART') {
          steps {
            dir('CART') {
              sh '''
                cd terraform-mutable
                make dev-apply
              '''
            }
          }
        }

    }
  }


